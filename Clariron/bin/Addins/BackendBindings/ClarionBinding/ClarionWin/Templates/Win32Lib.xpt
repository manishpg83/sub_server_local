<?xml version="1.0"?>
<Template originator   = "SoftVelocity">

	<!-- Template Header -->
	<TemplateConfiguration>
		<Name>${res:Clarion.Templates.Project.Win32Lib.Name}</Name>
		<Category>Clarion for Windows</Category>
		<Icon>CW.Project.EmptyProject</Icon>
		<Description>${res:Clarion.Templates.Project.Win32Lib.Description}</Description>
	</TemplateConfiguration>

	<Actions>
		<Open filename = "${ProjectName}.clw"/>
	</Actions>

	<Project name = "${ProjectName}" directory = "." language = "Clarion">
		<PropertyGroup>
			<OutputType>Lib</OutputType>
		</PropertyGroup>

		<Files>
			<File name="${ProjectName}.clw">
				<![CDATA[
  PROGRAM

${StandardHeader.Clarion}

  MAP
  END

  CODE
]]>
			</File>
		</Files>
	</Project>
</Template>

